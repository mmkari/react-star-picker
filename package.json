{
  "name": "react-star-picker",
  "version": "1.0.2",
  "description": "A star-rating component made with React",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "check-types": "tsc",
    "flow": "flow",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf dist/*",
    "build": "./node_modules/.bin/babel src --out-dir dist  --extensions '.js,.jsx,.ts,.tsx'",
    "build_old": "./node_modules/.bin/babel src --out-dir dist",
    "start:dev": "node config/webpack-dev-server.js",
    "clean-examples": "rm -rf dist-examples/*",
    "build-pages": "npm run clean-examples && webpack --config config/webpack-prod-config.js",
    "start:prod": "npm run build-pages && serve dist-examples",
    "start:prod-nb": "serve dist-examples",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "clean-styleguide": "rm -rf styleguidistHtml/*",
    "predeploy": "npm run clean-styleguide && npm run styleguide:build",
    "deploy": "gh-pages -d styleguidistHtml"
  },
  "homepage": "https://mmkari.github.io/react-star-picker/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmkari/react-star-picker.git"
  },
  "keywords": [
    "star",
    "rating",
    "input",
    "review",
    "ratings",
    "react"
  ],
  "author": "Miko Kari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmkari/react-star-picker/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "acorn": "^6.3.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.2.0",
    "flow-bin": "^0.107.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "path": "^0.12.7",
    "polished": "^3.4.1",
    "prettier-eslint": "^9.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.13",
    "react-styleguidist": "^10.3.2",
    "serve": "^11.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "styled-components": "^4.4.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.40.2",
    "webpack-command": "^0.5.0",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "peerDependencies": {
    "polished": ">= 3.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": ">= 4.4.0"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
