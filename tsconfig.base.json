{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "jsx": "react",
    "declaration": true,
    "outDir": "./lib/esm/dts",
    "rootDir": "./src",
    "skipLibCheck": true,

    /* Strict Type-Checking Options */
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,

    /* Module Resolution Options */
    "esModuleInterop": true
  },
  "exclude": ["./docusaurus/**/*", "./dist/**/*"]
}
